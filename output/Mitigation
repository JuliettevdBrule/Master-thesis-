# CBR_PCC ["Burden sharing"] = "GF"
# CBR_AP = pd.read_csv('CBR_AP.csv').drop(['Unnamed: 0'], axis =1).melt("Region")
# CBR_AP ["Burden sharing"] = "GF"
# CBR_GDR = pd.read_csv('CBR_GDR.csv').drop(['Unnamed: 0'], axis =1).melt("Region")
