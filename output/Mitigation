CBR_mit = CBR_mit.drop(['Unnamed: 0'], axis =1)
CBR_mit[ "Burden sharing"] = 'none'
CBR_mit['variable'] = 'CBR'
CBR_mit.rename(columns = { 'CBR' :'value' }, inplace =True )
CBR_mit.iloc[:,[0,3,1,2]]



