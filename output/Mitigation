sums_graph = pd.concat(sums, axis = 0)

#sums_graph = sums_graph[sums_graph.variable<'2101']


sums_graph
fig0 = px.scatter(
    sums_graph, 
    x='Region',
    y= 'value',
    color="Burden sharing",
    title = ' total mitigation costs per burden sharing regime 2020-2150',
    
