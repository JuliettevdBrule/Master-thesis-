sum_costs_GF = sum_costs_GF.sum(axis=1).to_frame().rename(columns = { 0: 'values'})
sum_costs_GF = (sum_costs_GF/sum_GDP_2100).reset_index()#.melt('Region').drop(['variable'], axis =1)
