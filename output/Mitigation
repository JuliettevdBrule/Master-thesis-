IEPC_al_pc_sum = IEPC_al_pc_sum.pivot(index='Region', columns='variable')
IEPC_al_pc_sum = IEPC_al_pc_sum.sum(axis=1).reset_index().melt("Region").drop(['variable'], axis =1)
IEPC_al_pc_sum ["Burden sharing"] = "IEPC"
                                
                                
