CBR_GDR ["Burden sharing regime"] = 'GDR'


frames = [CBR_GF, CBR_IEPC, CBR_PCC, CBR_AP, CBR_GDR, CBR_mit]
CBRs = pd.concat(frames)
CBRs.to_csv('CBRs.csv')
