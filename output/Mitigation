Mit_per_glo = (mitigation_costs/gdp_df).reset_index().melt('Region')
Mit_per_glo ['Burden sharing'] = 'none'

frames1 = [ GF_per_glo, IEPC_per_glo, PCC_per_glo, AP_per_glo, GDR_per_glo, Mit_per_glo]
