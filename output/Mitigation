MGCS_Plots= pd.concat(MGCS_Plots, axis=0)
MGCS_Plots

fig = px.line(
    MGCS_Plots,
    x='variable',
    y= 'value',
    line_group="Region",
    color="Burden sharing",
    title = 'Mitigation costs per burden sharing regime 2020-2150',
    facet_col = "Region",
    facet_col_wrap=5,
    height=1500
    
)
fig
