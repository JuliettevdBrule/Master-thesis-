#data MCGs
MCG_GF = pd.read_csv('MCG_GF.csv')
MCG_IEPC = pd.read_csv ('MCG_IEPC.csv')
MCG_PCC =  pd.read_csv ('MCG_PCC.csv')
MCG_AP = pd.read_csv ('MCP_AP.csv')
MCG_GDR = pd.read_csv ('MCG_GDR.csv')

MCG_GF_sum = MCG_GF
MCG_IEPC_sum = MCG_IEPC
MCG_PCC_sum = MCG_PCC
MCG_AP_sum = MCG_AP
MCG_GDR_sum = MCG_GDR_


