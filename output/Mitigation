frames = [MCG_APsum, MCG_GDRsum, MCG_GFsum, MCG_IEPCsum, MCG_PCCsum] 
MCGs = pd.concat(frames, axis=1)
MCGs = MCGs.set_axis([ 'GF', 'GDR','PCC', 'IEPC', 'AP'], axis= 1)
MCGs = MCGs.reset_index()

MCGs
