CBRs = CBRs.drop(['Region 2', 'Region 1', 'Region'], axis =1)
CBRs = px.scatter(MCGs,
                  x="Region 3",
                 y= ['CBR_AP', 'CBR_GF', 'CBR_IEPC', 'CBR_PCC'],
                  title = 'Cost Benefit Ratio per region per burden sharing regime')


CBRs.update_xaxes(
    title = "Region"
)

CBRs.update_yaxes(
    title = "Cost Benefit Ratio",
    #range = [-10,3],
)
