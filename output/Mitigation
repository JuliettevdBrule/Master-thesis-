MCG_PCC_sum = MCG_PCC_ind.sum(axis = 1)
MCG_PCC_sum= MCG_PCC_sum.to_frame()
MCG_PCC_sum = MCG_PCC_sum.reset_index()
MCG_PCC_sum.columns = ['Region 2', 'MCG PCC']
