MCG_GF_fig = px.scatter(MCG_GF_sum, x="Region")
#MCG_IEPC_fig =  px.bar(MCG_IEPC, x="Region", y="year",)
#MCG_PCC_fig =  px.bar(MCG_PCC, x="Region", y="CBR",)
