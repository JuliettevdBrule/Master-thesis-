MCG_GDR = MCG_GDR.melt('Region')
MCG_GDR ["Burden sharing regime"] = 'GDR'
MCG_GF = MCG_GF.melt('Region')
MCG_GF ["Burden sharing regime"] = 'GF'
MCG_PCC = MCG_PCC.melt('Region')
MCG_PCC["Burden sharing regime"] = 'PCC'

