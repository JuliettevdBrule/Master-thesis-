    Reg_of_int,
    x='variable',
    y= 'value',
    line_group="Region",
    color="Burden sharing",
    title = 'Allowances per burden sharing regime for 2020-2100 in GtCO2/yr',
    facet_col = "Region",
    facet_col_wrap=5,
    height=350
    
