PPC_al_pc_sum10 = PPC_al_pc_sum10.pivot(index='Region', columns='variable') 
PPC_al_pc_sum10 = PPC_al_pc_sum10.sum(axis=1).to_frame().reset_index().melt('Region').drop(['variable'], axis =1)
PPC_al_pc_sum10 ["Burden sharing"] = "PCC"

