def npv(dataframe):
    NPV_values=pd.Series({
        column: npf.npv(discount_rate, values)
        for column, values in dataframe.iteritems()
    })
    return NPV_values

Mit= npv(mitigation_costs)
AD = npv(avoided_damages)

