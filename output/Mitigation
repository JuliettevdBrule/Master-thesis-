global_GF ['Burden sharing'] = 'GF'
global_IEPC = costs_IEPC.sum(axis=0).to_frame()
global_IEPC ['Burden sharing'] = 'IEPC'
global_PCC = costs_PCC.sum(axis=0).to_frame()
global_PCC ['Burden sharing'] = 'PCC'
