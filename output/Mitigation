sum_mitig = sum_mitig.pivot(index='Region', columns='variable')
sum_mitig= sum_mitig.sum(axis=1).to_frame().reset_index().melt('Region').drop(['variable'], axis =1)
sum_mitig ["Burden sharing"] = "none"

