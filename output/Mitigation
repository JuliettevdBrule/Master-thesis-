# MCG_AP ["Burden sharing regime"] = 'AP'
# MCG_IEPC = MCG_IEPC.melt('Region')
# MCG_IEPC ["Burden sharing regime"] = 'IEPC'
# MCG_GDR = MCG_GDR.melt('Region')
