# sum_costs_AP = sum_costs_AP.pivot(index='Region', columns='variable')
# sum_costs_AP = sum_costs_AP.sum(axis=1).to_frame().reset_index().melt('Region').drop(['variable'], axis =1)
# sum_costs_AP ["Burden sharing"] = "AP"

