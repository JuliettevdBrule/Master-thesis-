costs_IEPC = costs_IEPC.reset_index().melt('Region')
costs_PCC = costs_PCC.reset_index().melt('Region')
costs_AP = costs_AP.reset_index().melt('Region')
costs_GDR = costs_GDR.reset_index().melt('Region')

