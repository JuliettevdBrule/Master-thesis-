al_mit_pc_2100 =al_mit_pc_2100.pivot(index='Region', columns='variable') 
al_mit_pc_2100 =al_mit_pc_2100.sum(axis=1).to_frame().reset_index().melt('Region').drop(['variable'], axis =1)
al_mit_pc_2100 ["Burden sharing"] = "none"


