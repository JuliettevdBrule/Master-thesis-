#NPV 

discount_rate= 0.03 

def npv(dataframe):
    NPV_values=pd.Series({
        region: npf.npv(discount_rate, row)
        for region, row in dataframe.iterrows()
    })
    return NPV_values


IEPC_NPV= npv(IPC_mitigationcosts)
AD = npv(avoided_damages)

