global_emissions_2050 = global_emissions.loc['2020':'2050',:]

sub = global_baseline_2050- global_emissions_2050
sub = sub.reset_index()
sub.drop(sub.columns[0], axis =1, inplace = True)

RA = sub('emissions').multiply(0,5)
