MCG_AP_sum = MCG_AP_ind.sum(axis = 1)
MCG_AP_sum= MCG_AP_sum.to_frame()
MCG_AP_sum = MCG_AP_sum.reset_index()
MCG_AP_sum.columns = ['Region 1', 'MCG AP']
