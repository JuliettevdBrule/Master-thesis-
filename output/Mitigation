# plots costs

costs_GF =costs_GF.reset_index().melt('Region')
costs_GF ["Burden sharing"] = "GF"
costs_IEPC = costs_IEPC.reset_index().melt('Region')
costs_IEPC ["Burden sharing"] = "IEPC"
