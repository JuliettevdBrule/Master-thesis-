CBR_AP = pd.read_csv ('CBR_AP.csv')
CBR_IEPC = pd.read_csv ('CBR_IEPC.csv')
CBR_PCC = pd.read_csv ('CBR_PCC.csv')
CBR_GDR = pd.read_csv ('CBR_GDR.csv')

