global_AP ['Burden sharing'] = 'AP'
global_GDR = costs_AP.sum(axis=0).to_frame()
global_GDR ['Burden sharing'] = 'GDR'
global_MIT = costs_AP.sum(axis=0).to_frame()
global_MIT ['Burden sharing'] = 'none'
global_gdp = GDP.sum(axis= 0).to_frame()

