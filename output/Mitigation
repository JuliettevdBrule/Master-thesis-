sum_costs_GF = sum_costs_GF.join(Image_region)
sum_costs_GF = sum_costs_GF.sort_values(by = 'IMAGE_region_num')##drop(columns = ['IMAGE_region_num'], axis =1)
#sum_costs_GF ["Burden sharing"] = "GF"

